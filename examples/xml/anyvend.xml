<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
    <properties>
        <graphics key="autolayoutconstraints" value="false"/>
        <graphics key="legendautolayout" value="true"/>
        <graphics key="showshortnames" value="false"/>
        <graphics key="layout" value="horizontal"/>
        <graphics key="showcollapsedconstraints" value="true"/>
        <graphics key="legendhidden" value="false"/>
        <graphics key="layoutalgorithm" value="1"/>
    </properties>
    <struct>
        <and abstract="true" mandatory="true" name="AnyVend">
            <graphics key="collapsed" value="false"/>
            <or abstract="true" mandatory="true" name="GUI">
                <feature name="WebInterface"/>
                <alt abstract="true" name="javaGUI">
                    <feature name="SWING"/>
                    <feature name="javaFX"/>
                </alt>
            </or>
            <or abstract="true" mandatory="true" name="Interaction">
                <feature name="API"/>
                <feature name="Analouge"/>
            </or>
            <or abstract="true" mandatory="true" name="Caching">
                <or abstract="true" name="Physical">
                    <feature name="Euro"/>
                    <feature name="Dollar"/>
                </or>
                <or abstract="true" name="Crypto">
                    <feature name="BTC"/>
                    <feature name="Monero"/>
                    <feature name="Doge"/>
                </or>
            </or>
            <feature name="Cooling"/>
            <and abstract="true" mandatory="true" name="Goods">
                <feature name="Ice"/>
                <or abstract="true" name="Drinks">
                    <graphics key="collapsed" value="false"/>
                    <and abstract="true" name="ALC">
                        <feature name="Beer"/>
                        <feature name="Wine"/>
                    </and>
                    <and abstract="true" name="NonALC">
                        <feature name="Cola"/>
                        <feature name="Water"/>
                    </and>
                </or>
                <or abstract="true" name="Snacks">
                    <feature name="Chips"/>
                    <feature name="Slickers"/>
                </or>
                <or abstract="true" name="Food">
                    <graphics key="collapsed" value="false"/>
                    <feature name="Pizza"/>
                    <feature name="Burger"/>
                    <feature name="Fries"/>
                    <feature name="Pasta"/>
                    <feature name="Doener"/>
                </or>
            </and>
            <feature name="AgeVerification"/>
            <feature name="Heating"/>
        </and>
    </struct>
    <constraints>
        <rule>
            <imp>
                <var>Heating</var>
                <not>
                    <var>Cooling</var>
                </not>
            </imp>
        </rule>
        <rule>
            <imp>
                <disj>
                    <var>Drinks</var>
                    <var>Ice</var>
                </disj>
                <var>Cooling</var>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>ALC</var>
                <var>AgeVerification</var>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>Food</var>
                <var>Heating</var>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>API</var>
                <var>WebInterface</var>
            </imp>
        </rule>
    </constraints>
</featureModel>